<!--dogparks.vtl-->
<h2>All the parks</h2>

#foreach($location in $locations)
  <ul>
    <li><a href="/locations/$location.getId()">$location.getNeighborhood()</a>
      <ul>
        #foreach($dogpark in $dogparks)
          #if ($location.getId() == $dogpark.getLocationId())
            <li><a href="/locations/$location.getId()/dogparks/$dogpark.getId()">$dogpark.getName()</li></a>
          #end
        #end
      </ul>
    </li>
  </ul>
#end
